{
    "executionRoleArn": " arn:aws:iam::368772107083:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "nestjs",
            "image": "$ECR_URI:$IMAGE_TAG",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "containerPort": 3000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}
